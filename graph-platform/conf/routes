# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# For dev machines
GET     /assets/*file                                                               controllers.Assets.at(path="/public", file)

# OPPM
GET     /sport/oppm/:year/:month/:day/$home<[\w\d-\.]+>-v-$away<[\w\d-\.]+>         controllers.MatchController.renderMatch(year,month,day,home,away)
